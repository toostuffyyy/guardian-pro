<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:TerminalEmployeeGeneralDivision.Views"
        xmlns:viewModels="clr-namespace:TerminalEmployeeGeneralDivision.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:DataType="viewModels:VisitorsOrderViewModel"
        x:Class="TerminalEmployeeGeneralDivision.Views.VisitorsOrderWindow"
        Title="VisitorsOrderWindow">
    <DataGrid Items="{Binding OrderDto.Visitors}">
        <DataGrid.Columns>
            <DataGridTextColumn Header="Email" Binding="{Binding Email}" />
        </DataGrid.Columns>
    </DataGrid>
</Window>
